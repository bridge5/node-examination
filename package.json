    "mongoose": "^5.7.11",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "jest": "24.9.0",
    "mockgoose": "^8.0.4",
    "supertest": "4.0.2"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
