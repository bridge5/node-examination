{
  "private": true,
  "name": "node-examination",
  "version": "0.0.1",
  "description": "Building a RESTful CRUD API with Node.js, Express/Koa and MongoDB.",
  "main": "server.js",
  "scripts": {
    "start": "set NODE_ENV=development && node server.js",
    "start:prod": "set NODE_ENV=production && node server.js",
    "test": "set NODE_ENV=test && yarn && jest ./test/api/*  --detectOpenHandles"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
